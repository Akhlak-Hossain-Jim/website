  "source": "// SPDX-License-Identifier: UNLICENSED\npragma solidity >=0.7.0 <0.9.0;\n\ncontract comdexAirdrop{\n\n    event MagicTxEvent(address sender, uint256 height, string xprtAddress, string accountAddress);\n\n    /**\n     * @dev emits the event for xprt address\n     */\n    function MagicTx(string memory xprtAddress, string memory accountAddress) public {\n        emit MagicTxEvent(msg.sender, block.number, xprtAddress, accountAddress);\n    }\n}",
